<% include ./partials/_header %>
<h2> Index Page </h2>
    <ul>
      <% keys = Object.keys(urls) %>     
        <% keys.forEach(function(key) { %>
       <li> 
        <%= key %> --> 
          <a href=" <%= urls[key] %> "> <%= urls[key] %> </a> 
          <a href="http://localhost:8080/urls/<%= key %>"> Edit </a> 
          <form method="POST" action="/urls/<%= key %>/delete">
          <input type="submit" value="Delete"> 
        </form>
       </li>
       <br>
        <% }); %>
    </ul>
    <a href="http://localhost:8080/urls/new"> Shorten a new URL </a>
</body>
</html>